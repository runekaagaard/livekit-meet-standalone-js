{
  "name": "livekit-meet-standalone",
  "version": "1.0.0",
  "type": "module",
  "description": "A standalone javascript implementation of the livekit meeting room. Just include a js script and off we go :)",
  "source": "livekit-meet-standalone.jsx",
  "targets": {
    "watch": {
      "source": ["livekit-meet-standalone.jsx"],
      "distDir": "dist/watch",
      "outputFormat": "global",
      "optimize": false,
      "sourceMap": true
    },
    "dev": {
      "source": ["dev.js"],
      "distDir": "dist/dev",
      "outputFormat": "global",
      "optimize": false,
      "sourceMap": true
    },
    "prod": {
      "source": ["livekit-meet-standalone.jsx"],
      "distDir": "dist/prod",
      "outputFormat": "global",
      "optimize": true,
      "sourceMap": false
    }
  },
  "scripts": {
    "watch": "parcel watch --target watch",
    "dev": "parcel build --target dev --no-source-maps --no-cache",
    "build": "parcel build --target prod --no-source-maps --no-cache",
    "serve": "serve"
  },
  "repository": {
    "type": "git",
    "url": "A standalone javascript implementation of the livekit meeting room. Just include a js script and off we go :)"
  },
  "keywords": [
    "livekit",
    "meet",
    "js"
  ],
  "author": "Rune Kaagaard",
  "license": "MPL-2.0",
  "dependencies": {
    "@livekit/components-react": "^2.6.11",
    "@livekit/components-styles": "^1.1.4",
    "livekit-server-sdk": "^2.9.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "parcel": "^2.13.3",
    "process": "^0.11.10"
  }
}
